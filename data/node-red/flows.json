    {
        "id": "5b32fc5c31804444",
        "type": "function",
        "z": "0eedd753f361bb4c",
        "name": "swings",
        "func": "const ts = msg.payload;\nconst swings = [\"swing1\", \"swing2\", \"swing3\", \"swing4\", \"swing5\", \"swing6\"];\n\nconst l = 2.5; // length of the swing in meters\nconst g = 9.81; // acceleration due to gravity in m/s^2\n\nlet msgs = [];\n\nfor (const swing of swings) {\n    let msg = {};\n    msg.payload = {};\n    msg.payload['ay'] = 0; // we assume y always 0\n    msg.payload['ts'] = ts;\n    msg.topic = swing\n\n    const start = flow.get(`${swing}_start`) || 0;\n    if (start == 0) {\n        // swing has not yet started\n        msg.payload['ax'] = 0;\n        msg.payload['az'] = g;\n    } else {\n        const theta0 = flow.get(`${swing}_theta0`) * Math.PI / 180 || 0.25 * Math.PI;  // convert to rad, default 45Â°\n        const t = (ts - start) / 1000; // convert to s\n\n        // calculate the current angle based on the time\n        const theta = theta0 * Math.cos(Math.sqrt(g / l) * t); \n\n        msg.payload['ax'] = -g * theta;\n        msg.payload['az'] = g * Math.cos(theta);\n    }\n    msgs.push(msg)\n}\n\nreturn [msgs];",
